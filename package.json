{
  "name": "darpsoft",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android ",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build": "SET ENVFILE=.env.production && react-native run-android --variant=release",
    "android:device": "SET ENVFILE=.env.development && react-native run-android --port 8081",
    "build:local": "SET ENVFILE=.env.production.local && react-native run-android --variant=release",
    "start:reset": "react-native start --reset-cache",
    "clean": "cd android && ./gradlew clean && cd ..",
    "reverse": "adb -s emulator-5554 reverse tcp:8081 tcp:8081",
    "install": "SET ENVFILE=.env.development && cd android && ./gradlew.bat app:installDebug -PreactNativeDevServerPort=8081 && cd ..",
    "test": "jest",
    "adb:reset": "adb kill-server",
    "export": "cd android && SET ENVFILE=.env.production && ./gradlew bundleRelease && cd ..",
    "eslint": "eslint . --fix",
    "prettier": "prettier . --write --config ./.prettierrc.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.14.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.8",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "16.13.1",
    "react-hook-form": "^6.15.3",
    "react-native": "0.63.4",
    "react-native-config": "^1.4.2",
    "react-native-gesture-handler": "^1.10.2",
    "react-native-paper": "^4.7.2",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.17.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.0",
    "@babel/runtime": "^7.13.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.65.1",
    "react-test-renderer": "16.13.1",
    "prettier": "^2.2.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-standard": "^5.0.0",
    "babel-eslint": "^10.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
